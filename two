  LbCname:
    Type: "AWS::Route53::RecordSet"
    Properties:
      HostedZoneName: !Sub "${HostedZoneName}"
      Comment: !Sub "DNS name for {ProductName} LB."
      Name: !Sub "a${AssetInsightId}-${ProductName}-${ServiceEnvironment}-${AWS::Region}.${HostedZoneName}"
      Type: 'CNAME'
      TTL: '300'
      ResourceRecords:
        - !GetAtt "LoadBalancerForEDD.DNSName"
